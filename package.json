{
  "name": "d3-dynamic-graph",
  "type": "module",
  "version": "1.3.0",
  "description": "A high level API to create interactive, smoothly updating D3.js force-directed graph layout data visualizations",
  "files": [
    "dist"
  ],
  "module": "./dist/d3-dynamic-graph.js",
  "main": "./dist/d3-dynamic-graph.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/d3-dynamic-graph.js",
      "require": "./dist/d3-dynamic-graph.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite --port 3344 --host",
    "build": "vite build",
    "lint": "eslint src",
    "pub-patch": "npm version patch --force && npm publish",
    "pub-minor": "npm version minor --force && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidnmora/d3-dynamic-graph.git"
  },
  "keywords": [
    "data",
    "visualization",
    "d3",
    "force",
    "graph",
    "directed",
    "layout",
    "d3force",
    "network",
    "node",
    "link",
    "animated"
  ],
  "author": "David Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidnmora/d3-dynamic-graph/issues"
  },
  "homepage": "https://github.com/davidnmora/d3-dynamic-graph#readme",
  "dependencies": {},
  "peerDependencies": {
    "d3": "^7.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-formatter-summary-chart": "^0.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "terser": "^5.15.1",
    "vite": "^3.2.3"
  }
}
